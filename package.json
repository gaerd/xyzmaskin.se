{
  "name": "xyz-maskin-landing",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "cross-env ELEVENTY_ENV=dev npm-run-all --parallel dev:*",
    "dev:11ty": "eleventy --serve --quiet",
    "dev:css": "postcss src/styles/tailwind.css -o src/assets/site.css --watch",
    "build": "cross-env NODE_ENV=production npm-run-all build:css build:11ty",
    "build:11ty": "eleventy",
    "build:css": "postcss src/styles/tailwind.css -o src/assets/site.css",
    "serve": "eleventy --serve"
  },
  "devDependencies": {
    "@11ty/eleventy": "^3.0.0",
    "@tailwindcss/typography": "^0.5.19",
    "autoprefixer": "^10.4.20",
    "cross-env": "^7.0.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.41",
    "postcss-cli": "^11.0.0",
    "tailwindcss": "^3.4.13"
  }
}
