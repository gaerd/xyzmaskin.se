{
  "name": "xyz-maskin-landing",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "cross-env ELEVENTY_ENV=dev npm-run-all --parallel dev:*",
    "dev:11ty": "eleventy --serve --quiet",
    "dev:css": "postcss src/assets/styles.css -o _site/assets/styles.css --watch",
    "build": "cross-env NODE_ENV=production run-p build:*",
    "build:11ty": "eleventy",
    "build:css": "postcss src/assets/styles.css -o _site/assets/styles.css",
    "serve": "eleventy --serve"
  },
  "devDependencies": {
    "@11ty/eleventy": "^3.0.0",
    "autoprefixer": "^10.4.20",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.41",
    "postcss-cli": "^11.0.0",
    "tailwindcss": "^3.4.13",
    "cross-env": "^7.0.3"
  }
}
